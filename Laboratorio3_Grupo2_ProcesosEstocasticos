   "source": [
    "def permutation_id(matrix):\n",
    "    G = DiGraph()\n",
    "    n = matrix.shape[0]\n",
    "    for i in range(n):\n",
    "        for j in range(n):\n",
    "            if matrix[i, j] > 0:\n",
    "                G.add_edge(i, j)\n",
    "    sccs_data = list(strongly_connected_components(G))\n",
    "    permutation = []\n",
    "    for scc in sccs_data:\n",
    "        permutation.extend(sorted(scc))\n",
    "    return eye(n)[permutation].T, sccs_data, permutation"
   ]
  },
