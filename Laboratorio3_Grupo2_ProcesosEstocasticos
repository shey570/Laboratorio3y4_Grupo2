    "# se calcula un espacio generador de vectores estacionarios, almacenados en gen_esta\n",
    "U,S,V,gen_esta = estacionario(A4)\n",
    "print('Bases del espacio de vectores estacionarios \\n',gen_esta,'\\n ---')\n",
    "# se calcula una retraccion de una combinacion de la base calculada para asegurar que sea una distribución estocástica\n",
    "distpi = retraccion(gen_esta[0]+4*gen_esta[1])\n",
    "print('Vector estacionario estocástico\\n',distpi,'\\n ---')\n",
    "print('Norma 1 del vector\\n',norm(distpi,ord = 1),'\\n ---')\n",
    "# se calcula el error de una transicion sobre la distribucion estacionaria calculada para asergurar que es estacionaria\n",
    "error = distpi@A4-distpi\n",
    "error = error*(abs(error)>1e-11)\n",
    "print('Verificando que el vector es estacionario\\n',error,'\\n ---')"
