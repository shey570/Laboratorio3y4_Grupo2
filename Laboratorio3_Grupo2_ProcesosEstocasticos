   "source": [
    "# se establece un vector inicial para graficar la evolución del modelo 1 hasta 100 transiciones\n",
    "X1_0 = np.array([1/4,1/4,1/4,1/4])\n",
    "print('\\n ---')\n",
    "print('Vector inicial\\n',X1_0,'\\n ---')\n",
    "N = 100\n",
    "x = X1_0.T\n",
    "# el vector Xn almacena cada estado x(t) para t entre 0 y 99\n",
    "Xn = [x]\n",
    "for i in range(N):\n",
    "    X = x@A1\n",
    "    y = x\n",
    "    x = X\n",
    "    Xn.append(X)\n",
    "print('Último vector generado\\n',X,'\\n ---')\n",
    "print('Verificando que el último vector es estacionario\\n',X@A1-X,'\\n ---')\n",
    "# Xn se transpone para poder graficarlo en python\n",
    "Xn = np.transpose(Xn)\n",
    "# print(np.shape(x))\n",
    "# print(np.shape(np.transpose(Xn)))\n",
    "plot(Xn.T)\n",
    "grid()\n",
    "legend(['0','1','2','3'])\n",
    "title('Evolución del Modelo 1')\n",
    "#savefig('Modelo1.png') # comando para guardar la figura del grafo"
