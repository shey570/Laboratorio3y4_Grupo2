   "cell_type": "code",
   "execution_count": 2,
   "id": "6c40c9f1",
   "metadata": {},
   "outputs": [],
   "source": [
    "#esta función construye una matriz para un modelo de ruina del jugador\n",
    "#recibe el estado de fortuna máxima y la probabilidad de éxito\n",
    "def modelo_rdj(N,p=0.5):\n",
    "    A = zeros((N+1,N+1))\n",
    "    A[0,0] = A[N,N] = 1\n",
    "    for j in range(N-1):\n",
    "        A[j+1,j] = 1-p\n",
    "        A[j+1,j+2] = p\n",
    "    return A"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "07aaddb0",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Fortuna máxima esperada por el jugador\n",
    "N = 10\n",
    "\n",
    "# Horizonte de tiempo de la simulación\n",
    "T = 200"
   ]
  },
